<%= render partial: "layouts/header" %>
<div id="page-wrapper" style="min-height: 650px;">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
      <div class="row"></div>
        <form class="horizontal-form" role="form">
            <div class="form-group">
              <div class="col-md-3">
                <h3>Hora:</h3>
              </div>
              <div class="col-md-5">
                <div class="input-group clockpicker">
                    <input type="text" class="form-control timerInput" value="09:30">
                    <span class="input-group-addon">
                        <span class="btn-sm glyphicon glyphicon-time"></span>
                    </span>
                </div>
              </div>
            </div>
            <div class="form-group">
                  <div class="col-md-3">
                    <h3>Númeración</h3>
                  </div>
                  <div class="col-md-5">
                    <input type="number" class="form-control" placeholder="Número de pasajero">
                  </div>
            </div>
            <div class="form-group">
                  <div class="col-md-3">
                    <h3>ID AutoBús</h3>
                  </div>
                  <div class="col-md-5">
                    <input type="number" class="form-control" placeholder="ID del Autobús">
                  </div>
            </div>
            <button type="submit" class="pull-right btn-lg btn-primary">Submit Button</button>
        </form>
      </div>
      <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
  </div>
  <!-- /.container-fluid -->
</div>