<div id="page-wrapper" style="min-height: 650px;">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <h1 class="page-header"> <%= link_to "Lista de datos", data_list_path %>
        </h1>
      </div>
      <!-- /.col-lg-12 -->
    </div>
    <div id='search-block' class="row">
      <div class="col-sm-4">
        <div class="row">
          <div class="col-sm-3"></div>
          <div class="col-sm-3">
            <p>Bus</p>
          </div>
          <div class="col-sm-6">
            <div class="select-style">
              <select name="" id="">
                <option value="volvo">001</option>
                <option value="saab">002</option>
                <option value="mercedes">003</option>
              </select>
            </div>
          </div>
        </div>
      </div>
      <div class="col-sm-4">
        <div class="row">
          <div class="col-sm-3">
            <p>Fecha</p>
          </div>
          <div class="col-sm-9">
            <input id="date-route-bus" type="text">
          </div>
        </div>
      </div>
      <div class="col-sm-4">
        <button type="button" class="btn btn-primary full-length">Ingresar</button>
      </div>
      <!-- /.col-lg-12 -->
    </div>

    <div class="row">
      <div class="col-lg-12">
        <div class="records-display">
          <div class="panel-body">
            <div class="dataTable_wrapper">
              <table class="table table-striped table-bordered table-hover" id="records-table">
              </table>
            </div>
          </div>
        <!-- /.records_display -->
        </div>
      <!-- /.col-lg-12 -->
      </div>
    </div>
  </div>
</div>
