           </table>
         </div>
         <!-- /.table-responsive -->
         <div class="well">
          <h4>Información de Tablas para  <%= (l Time.now, format: :short).capitalize %></h4>
        </div>
      </div>
      <!-- /.panel-body -->
    </div>
    <!-- /.panel -->
  </div>
  <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
</div>
<!-- /.container-fluid -->
